[Unit]
Description=Gunicorn 2-hbnb systemd service to always start my web app when the server starts
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4/
ExecStart=/usr/local/bin/gunicorn -w 3 --bind 0.0.0.0:5003 --access-logfile /tmp/airbnb-access.log --error-logfile /tmp/airbnb-error.log web_dynamic.2-hbnb:app
Restart=always

[Install]
WantedBy=multi-user.target
